# Application
NEXT_PUBLIC_SITE_URL=https://at-solvexx.test
NEXT_PUBLIC_APP_NAME=At-Solvexx

# Database (Supabase)
DATABASE_URL=postgres://user:password@localhost:5432/portfolio_builder
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# Authentication
NEXTAUTH_SECRET=your-secret-key-here
NEXTAUTH_URL=http://localhost:3000

# Vercel Deployment
VERCEL_DEPLOY_HOOK=https://api.vercel.com/v1/integrations/deploy/...
VERCEL_PROJECT_ID=your-project-id
VERCEL_TEAM_ID=your-team-id

# Razorpay (Payment)
NEXT_PUBLIC_RAZORPAY_KEY_ID=rzp_test_xxxxx
RAZORPAY_KEY_SECRET=your-razorpay-secret

# Cloudflare (DNS)
CLOUDFLARE_TOKEN=your-cloudflare-token
CLOUDFLARE_ACCOUNT_ID=your-account-id
CLOUDFLARE_ZONE_ID=your-zone-id

# Email (Resend)
RESEND_API_KEY=your-resend-api-key

# Google reCAPTCHA v3
NEXT_PUBLIC_RECAPTCHA_SITE_KEY=your-recaptcha-site-key
RECAPTCHA_SECRET_KEY=your-recaptcha-secret-key

# Image Storage (Cloudinary)
NEXT_PUBLIC_CLOUDINARY_CLOUD_NAME=your-cloud-name
CLOUDINARY_API_KEY=your-api-key
CLOUDINARY_API_SECRET=your-api-secret

# Analytics (Plausible)
NEXT_PUBLIC_PLAUSIBLE_DOMAIN=yourdomain.com
PLAUSIBLE_API_KEY=your-api-key

# Admin
ADMIN_EMAIL=admin@at-solvexx.com
ADMIN_PASSWORD_HASH=your-hashed-password

# n8n Webhook
N8N_WEBHOOK_URL=http://localhost:5678/webhook/

# Optional: Sentry
NEXT_PUBLIC_SENTRY_DSN=your-sentry-dsn
